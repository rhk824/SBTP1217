<UserControl x:Class="SBTP.TPJtables.TPJ_table"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SBTP.TPJtables"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <DataGrid Name="DataGrid1" ItemsSource="{Binding}" CanUserAddRows="True" CanUserDeleteRows="False" PreviewKeyDown="DataGrid1_PreviewKeyDown" />
    </Grid>
</UserControl>

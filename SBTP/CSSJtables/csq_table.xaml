<UserControl x:Class="SBTP.CSSJtables.csq_table"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Loading="clr-namespace:SBTP.BLL"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <DockPanel>
        <DataGrid Name="DataGrid1" DockPanel.Dock="Left" CanUserResizeColumns="False"  ItemsSource="{Binding}"  CanUserAddRows="False" CanUserDeleteRows="False" PreviewKeyDown="DataGrid1_PreviewKeyDown" />
    </DockPanel>
        

</UserControl>

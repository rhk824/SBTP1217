                        <TextBlock Style="{StaticResource txt_header_2_style}" Grid.Row="1" Grid.Column="1">
                                <Run Text="视吸水指数（m" /><Run Text="3" BaselineAlignment="TextTop" FontSize="8" /><Run Text="/Mpa.d）"/>
                        </TextBlock>
                        <TextBlock Grid.Row="1" Grid.Column="2" Text="调剖层吸液分数（%）" Style="{StaticResource txt_header_2_style}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

